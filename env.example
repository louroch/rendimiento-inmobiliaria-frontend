# URL del backend API (Railway - Producción)
REACT_APP_BACKEND_URL=https://rendimiento-inmobiliaria-production.up.railway.app

# URL completa del API (Local - Desarrollo)
REACT_APP_API_URL=http://localhost:5000/api

# Configuración de entorno
REACT_APP_ENV=development

# Modo mantenimiento para agentes (true/false)
# Cuando está activado, los agentes verán un mensaje de sistema desactivado
# Los administradores seguirán teniendo acceso completo
REACT_APP_MAINTENANCE_MODE=false